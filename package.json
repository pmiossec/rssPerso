{
  "name": "rss-perso",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-notifications": "^1.7.4",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "sass": "^1.57.1"
  },
  "scripts": {
    "start": "concurrently \"npm:watch-css\" \"npm:start-ts\"",
    "start-ts": "react-scripts start",
    "build": "set PUBLIC_URL=.&& npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "sass src/:src/",
    "watch-css": "npm run build-css && sass src/:src/ --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
